{
  "name": "khody2012/laravel-bale",
  "authors": [
    {
      "name": "Mohammad Khodarahmpour",
      "email": "khodarahmpoor@gmail.com"
    }
  ],
  "homepage": "https://github.com/khody2012/laravel-bale",
  "type": "library",
  "description": "Laravel package to integrate with Bale messenger (bot API)",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Khody2012\\LaravelBale\\": "src/"
    }
